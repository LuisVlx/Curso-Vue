{"ast":null,"code":"import PokemonPicture from '@/components/PokemonPicture';\nimport PokemonOptions from '@/components/PokemonOptions';\nimport getPokemonOptions from '@/helpers/getPokemonOptions';\n//console.log(getPokemonOptions())\n\nexport default {\n  name: \"PokemonPage\",\n  components: {\n    PokemonPicture,\n    PokemonOptions\n  },\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false\n    };\n  },\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions();\n      const rndInt = Math.floor(Math.random() * 4);\n      this.pokemon = this.pokemonArr[rndInt];\n    },\n    checkAnswer(pokemonId) {\n      this.showPokemon = true;\n      console.log('PokemonPage llamado', pokemonId);\n    }\n  },\n  mounted() {\n    this.mixPokemonArray();\n  }\n};","map":{"version":3,"mappings":"AAWA,OAAOA,cAAa,MAAO,6BAA6B;AACxD,OAAOC,cAAa,MAAO,6BAA4B;AAEvD,OAAOC,iBAAgB,MAAO,6BAA4B;AAC1D;;AAEA,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRJ,cAAc;IACdC;EACJ,CAAC;EACDI,IAAI,GAAE;IACF,OAAM;MACFC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE,IAAI;MACbC,WAAW,EAAE;IACjB;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,eAAe,GAAE;MACnB,IAAI,CAACJ,UAAS,GAAI,MAAMJ,iBAAiB,EAAC;MAE1C,MAAMS,MAAK,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,EAAE,GAAC,CAAC;MACzC,IAAI,CAACP,OAAM,GAAI,IAAI,CAACD,UAAU,CAACK,MAAM;IACzC,CAAC;IACDI,WAAW,CAACC,SAAS,EAAC;MAClB,IAAI,CAACR,WAAU,GAAI,IAAG;MACtBS,OAAO,CAACC,GAAG,CAAC,qBAAqB,EAAEF,SAAS;IAChD;EACJ,CAAC;EACDG,OAAO,GAAG;IACN,IAAI,CAACT,eAAe,EAAC;EACzB;AACJ","names":["PokemonPicture","PokemonOptions","getPokemonOptions","name","components","data","pokemonArr","pokemon","showPokemon","methods","mixPokemonArray","rndInt","Math","floor","random","checkAnswer","pokemonId","console","log","mounted"],"sourceRoot":"","sources":["/home/des-admin/Documentos/CursoVue/04-pokemon-game/src/pages/PokemonPage.vue"],"sourcesContent":["<template>\n    <h1 v-if=\"!pokemon\"> Espere por favor... </h1>\n    <div v-else>\n        <h1>¿Quién es este pokémon?</h1>\n        <PokemonPicture :pokemonId=\"pokemon.id\" :showPokemon=\"showPokemon\"/>\n        <PokemonOptions :pokemons=\"pokemonArr\" @selection=\"checkAnswer()\"/>\n    </div>\n\n</template>\n\n<script>\nimport PokemonPicture from '@/components/PokemonPicture';\nimport PokemonOptions from '@/components/PokemonOptions'\n\nimport getPokemonOptions from '@/helpers/getPokemonOptions'\n//console.log(getPokemonOptions())\n\nexport default {\n    name: \"PokemonPage\",\n    components: {\n        PokemonPicture,\n        PokemonOptions\n    },\n    data(){\n        return{\n            pokemonArr: [],\n            pokemon: null,\n            showPokemon: false\n        }\n    },\n    methods: {\n        async mixPokemonArray(){\n            this.pokemonArr = await getPokemonOptions()\n\n            const rndInt = Math.floor(Math.random()*4)\n            this.pokemon = this.pokemonArr[rndInt]\n        },\n        checkAnswer(pokemonId){\n            this.showPokemon = true\n            console.log('PokemonPage llamado', pokemonId)\n        }\n    },\n    mounted() {\n        this.mixPokemonArray()\n    },\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}