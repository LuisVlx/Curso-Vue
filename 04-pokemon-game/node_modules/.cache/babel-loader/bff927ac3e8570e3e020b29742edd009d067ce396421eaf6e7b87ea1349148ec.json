{"ast":null,"code":"import PokemonPicture from '@/components/PokemonPicture';\nimport PokemonOptions from '@/components/PokemonOptions';\nimport getPokemonOptions from '@/helpers/getPokemonOptions';\n//console.log(getPokemonOptions())\n\nexport default {\n  name: \"PokemonPage\",\n  components: {\n    PokemonPicture,\n    PokemonOptions\n  },\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false,\n      showAnswer: false,\n      message: ''\n    };\n  },\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions();\n      const rndInt = Math.floor(Math.random() * 4);\n      this.pokemon = this.pokemonArr[rndInt];\n    },\n    checkAnswer(selectedId) {\n      this.showPokemon = true;\n      this.showAnswer = true;\n      if (selectedId === this.pokemon.id) {\n        this.message = `Correcto, ${this.pokemon.name}`;\n      } else {\n        this.message = `Oops, era ${this.pokemon.name}`;\n      }\n    },\n    newGame() {\n      this.pokemonArr = [];\n      this.pokemon = null;\n      this.showPokemon = false;\n      this.showAnswer = false;\n      this.message = '';\n    }\n  },\n  mounted() {\n    this.mixPokemonArray();\n  }\n};","map":{"version":3,"mappings":"AAsBA,OAAOA,cAAa,MAAO,6BAA6B;AACxD,OAAOC,cAAa,MAAO,6BAA4B;AAEvD,OAAOC,iBAAgB,MAAO,6BAA4B;AAC1D;;AAEA,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRJ,cAAc;IACdC;EACJ,CAAC;EACDI,IAAI,GAAE;IACF,OAAM;MACFC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE,IAAI;MACbC,WAAW,EAAE,KAAK;MAClBC,UAAU,EAAE,KAAK;MACjBC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,eAAe,GAAE;MACnB,IAAI,CAACN,UAAS,GAAI,MAAMJ,iBAAiB,EAAC;MAE1C,MAAMW,MAAK,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,EAAE,GAAC,CAAC;MACzC,IAAI,CAACT,OAAM,GAAI,IAAI,CAACD,UAAU,CAACO,MAAM;IACzC,CAAC;IACDI,WAAW,CAACC,UAAU,EAAC;MACnB,IAAI,CAACV,WAAU,GAAI,IAAG;MACtB,IAAI,CAACC,UAAS,GAAI,IAAG;MACrB,IAAGS,UAAS,KAAM,IAAI,CAACX,OAAO,CAACY,EAAE,EAAC;QAC9B,IAAI,CAACT,OAAM,GAAK,aAAY,IAAI,CAACH,OAAO,CAACJ,IAAK;M